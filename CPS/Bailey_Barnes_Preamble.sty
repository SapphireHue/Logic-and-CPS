	\vanswerswitch
		{\ver{#1}{#2}}
		{\ver{\scriptsize #2}{\scriptsize #1}}
}
\newcommand{\vgrans}[2]{ 							% graph answer key for multiple versions
	\vgranswerswitch
		{\ver{#1}{#2}}
		{\ver{#2}{#1}}
}

